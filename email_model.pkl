from sklearn.ensemble import RandomForestClassifier
import pickle
import numpy as np

# Sample training data (replace with actual data)
X_email_train = np.random.rand(100, 2)  # 100 samples, 2 features
y_email_train = np.random.randint(2, size=100)

# Train Email phishing detection model
email_model = RandomForestClassifier()
email_model.fit(X_email_train, y_email_train)

# Save the email model
with open("email_model.pkl", "wb") as email_model_file:
    pickle.dump(email_model, email_model_file)
